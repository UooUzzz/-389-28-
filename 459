class Solution {
  String findTheDifference(String s, String t) {
  Map<String, int> count = {};
  for (int i = 0; i < s.length; i++) {
    String char = s[i];
    count[char] = (count[char] ?? 0) + 1;
  }
  for (int i = 0; i < t.length; i++) {
    String char = t[i];
    count[char] = (count[char] ?? 0) - 1;
    if (count[char] == -1) {
      return char;
    }
  }
  return '';
}

}
